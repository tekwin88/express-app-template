<div class="container center1">
  <p>Welcome <%= currentUserLoggedIn %><p>
  <p>This is the new note page.</p>

  <form action="/notes/email" method="POST">
    <div class="form-group">
      <label for="noteTitle" class="control-label-white">Email</label>
      <input id="noteTitle" class="form-control1" type="email" name="email">
    </div>
    <div class="form-group">
      <label for="noteTitle" class="control-label-white">Title</label>
      <input id="noteTitle" class="form-control1" type="title" name="title">
    </div>
    <div class="form-group">
      <label for="noteBody" class="control-label-white">Body</label>
      <textarea class="form-control1" rows="5" id="noteBody" type="body" name="body"></textarea>
    </div>

    <input class="btn btn-primary" type="submit" value="Submit"></input>
    <button class="btn btn-danger btn-success pull-center"><a href='/'>Home</a></button>
    <button class="btn btn-danger btn-default pull-right"><span class="glyphicon glyphicon-remove"></span><a href='/'> Cancel</a></button>
  </form>
</div>
